    "@types/node": "^12.11.1",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "karma-coverage": "~2.0.3",
    "typescript": "~4.8.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
